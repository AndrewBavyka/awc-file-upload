{
  "$schema": "https://raw.githubusercontent.com/lit/lit/main/packages/localize-tools/config.schema.json",
  "sourceLocale": "en",
  "targetLocales": ["ru_RU"],
  "tsConfig": "tsconfig.json",
  "output": {
    "mode": "runtime",
    "outputDir": "./src/awc-file-upload/generated/locales",
    "localeCodesModule": "./src/awc-file-upload/generated/locale-codes.js"
  },
  "interchange": {
    "format": "xliff",
    "xliffDir": "./xliff/"
  }
}   